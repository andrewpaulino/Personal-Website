        sr.reveal('.ptext', {
          duration: 2000,
          origin:'right',
          distance:'300px'
        });
        sr.reveal('.sing', {
          duration: 2000,
          origin:'left',
          distance:'300px'
        });
        sr.reveal('#left', {
          duration: 2000,
          delay: 1000,
          origin:'left'
        });
        sr.reveal('#right', {
          duration: 2000,
          delay: 1000,
          origin:'right'
        });
        sr.reveal('.right', {
          duration: 2000,
